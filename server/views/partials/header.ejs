<header>
	<div class="logo">
		<h1 class="logo-text">MovieSite</h1>
	</div>
	<nav>
		<ul>
			<li><a href="/">Movies</a></li>
			<li><a href="/search">Search</a></li>
			<li><a href="/contact">Contact Us</a></li>
		</ul>
		
	</nav> 
</header>
<section class="hero">
	<% if(typeof movies != 'undefined'){ %>
		<% movies.slice().forEach(movie =>{ %>
		<div class="hero-info">
			<h1 class="movie-title-big"><%= movie.movie_title %></h1>
			<p class="movie-genre"><%= movie.movie_genre %></p>
			<a href="/info/<%= movie.movie_id %>"><button class="movie-details">Learn More</button></a>
		</div>
		<% }); %>
	<% } %>
</section>